{
  "$schema": "https://raw.githubusercontent.com/nicegui/nicegui/main/nicegui/json_schemas/tauri-conf-v2.json",
  "productName": "Fluffwire",
  "version": "0.1.0",
  "identifier": "com.fluffwire.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Fluffwire",
        "width": 1280,
        "height": 800,
        "minWidth": 940,
        "minHeight": 560
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
